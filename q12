#include <iostream>
#include<math.h>
using namespace std;
int main() {
    int a,b,c,d,r1,r2;
   cout<<"Enter numbers a ,b,c for quadratic equation";
   cin>>a>>b>>c;
  d=b*b-4*a*c;
  if(d>0){
      r1=(-b+sqrt(d))/(2*a);
      r2=(-b-sqrt(d))/(2*a);
  cout<<"roots are real and distinct with values"<<r1<<endl<<r2;
  }
  else if(d=0){
      r1=(-b+sqrt(d))/(2*a);
  cout<<"roots are real and equal with value"<<r1; 
  }
  else
  {
      cout<<"No real roots exist";
  }
    return 0;
}
