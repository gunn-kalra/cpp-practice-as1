#include<iostream>
using namespace std;
int main(){
    int a[5];
    int i=0;
    cout<<"Enter array elements";
    for(;i<5;i++)
    {
        cin>>a[i];
    }
    int max=a[0];
    int s=a[0];
    for(i=0;i<5;i++)
    {
        if(a[i]>max){
        s=max;
        max=a[i];}
    }
    /* for(i=0;i<5;i++)
    {
        if(a[i]>s && a[i]<max)
        s=a[i];
    }*/
    cout<<"Second largest element:"<<s;
    return 0;
}
